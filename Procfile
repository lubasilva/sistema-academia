release: composer install --no-dev --optimize-autoloader --ignore-platform-reqs --no-scripts && php artisan key:generate --force 2>/dev/null || true && php artisan config:clear && php artisan view:clear && php artisan migrate --force 2>/dev/null || true && php artisan db:seed --force 2>/dev/null || true && php artisan storage:link 2>/dev/null || true
web: php artisan optimize && php -S 0.0.0.0:${PORT:-8000} -t public
